@prefix lv2:  <http://lv2plug.in/ns/lv2core#>.
@prefix doap: <http://usefulinc.com/ns/doap#>.
@prefix epp: <http://lv2plug.in/ns/ext/port-props#>.
@prefix foaf: <http://xmlns.com/foaf/0.1/>.
@prefix mod: <http://moddevices.com/ns/mod#>.
@prefix modgui: <http://moddevices.com/ns/modgui#>.
@prefix rdf:  <http://www.w3.org/1999/02/22-rdf-syntax-ns#>.
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#>.
@prefix atom: <http://lv2plug.in/ns/ext/atom#> .
@prefix midi: <http://lv2plug.in/ns/ext/midi#> .
@prefix urid: <http://lv2plug.in/ns/ext/urid#> .

<http://moddevices.com/plugins/mod-devel/hmi-widgets-individual-tests>
    a lv2:Plugin, mod:UtilityPlugin;

    doap:name "HMI Widgets Individual Tests";
    doap:license "GPL v2+";
    doap:developer [
        foaf:name "Jan Janssen";
        foaf:homepage <>;
        foaf:mbox <mailto:jan@moddevices.com>;
    ];

    doap:maintainer [
        foaf:name "MOD";
        foaf:homepage <http://moddevices.com>;
        foaf:mbox <mailto:jan@moddevices.com>;
    ];

    lv2:requiredFeature <http://moddevices.com/ns/hmi#WidgetControl>;
    lv2:extensionData <http://moddevices.com/ns/hmi#PluginNotification>;

    lv2:minorVersion 3;
    lv2:microVersion 0;

    rdfs:comment """
A plugin for demonsration perposes on how to use the MOD HMI widgets functionality, individually for each type
    """;

    lv2:port [
        a lv2:InputPort, lv2:ControlPort;
        lv2:index 0;
        lv2:symbol "BYPASS_LED";
        lv2:name "BYPASS LED Test";
        lv2:portProperty lv2:integer, lv2:toggled;
        lv2:default 1;
        lv2:minimum 0;
        lv2:maximum 1;
        lv2:designation lv2:enabled;
    ] , [
        a lv2:InputPort, lv2:ControlPort;
        lv2:index 1;
        lv2:symbol "FOOT_LED";
        lv2:name "FOOT LED Test";
        lv2:portProperty lv2:integer, lv2:toggled;
        lv2:default 0;
        lv2:minimum 0;
        lv2:maximum 1;
    ] , [
        a lv2:InputPort, lv2:ControlPort;
        lv2:index 2;
        lv2:symbol "FOOT_LABEL";
        lv2:name "FOOT Label Test";
        lv2:portProperty lv2:integer, lv2:toggled;
        lv2:default 0;
        lv2:minimum 0;
        lv2:maximum 1;
    ] , [
        a lv2:InputPort ,
        lv2:ControlPort ;
        lv2:index 3 ;
        lv2:symbol "KNOB_LABEL" ;
        lv2:name "Knob Label Test";
        lv2:default 1.0 ;
        lv2:minimum 0.0 ;
        lv2:maximum 10.0 ;
    ] , [
        a lv2:InputPort ,
        lv2:ControlPort ;
        lv2:index 4 ;
        lv2:symbol "KNOB_INDICATOR" ;
        lv2:name "Knob Indicator Test";
        lv2:default 1.0 ;
        lv2:minimum 0.0 ;
        lv2:maximum 10.0 ;
    ] , [
        a lv2:InputPort ,
        lv2:ControlPort ;
        lv2:index 5 ;
        lv2:symbol "KNOB_VALUE" ;
        lv2:name "Knob Value Test";
        lv2:default 1.0 ;
        lv2:minimum 0.0 ;
        lv2:maximum 10.0 ;
    ] , [
        a lv2:InputPort ,
        lv2:ControlPort ;
        lv2:index 6 ;
        lv2:symbol "KNOB_UNIT" ;
        lv2:name "Knob Unit Test";
        lv2:default 1.0 ;
        lv2:minimum 0.0 ;
        lv2:maximum 10.0 ;
    ]
.
